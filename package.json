{
  "name": "default-backend-repo",
  "version": "1.0.0",
  "description": "Default settings for backend app using nodejs, typescript, eslint, prettier, webpack, nodemon, husky, and jest",
  "main": "src/server.ts",
  "author": "Allan Aoudji",
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode production --config webpack.config.ts",
    "dev": "nodemon",
    "lint": "eslint --no-error-on-unmatched-pattern \"src/**/*.{js,ts}\" \"__tests__/**/*.test.{js,ts}\"",
    "lint:fix": "eslint --fix --no-error-on-unmatched-pattern \"src/**/*.{js,jsx,ts,tsx}\" \"__tests__/**/*.test.{js,jsx,ts,tsx}\"",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --passWithNoTests",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.6",
    "@types/supertest": "^2.0.12",
    "@types/webpack-node-externals": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@webpack-cli/generators": "^3.0.1",
    "babel-cli": "^6.26.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.0",
    "jest": "^29.4.3",
    "lint-staged": "^13.1.2",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.4",
    "superagent": "^8.0.9",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.14.2",
    "typescript": "*",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2"
  }
}
